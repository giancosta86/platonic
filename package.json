{
  "name": "@giancosta86/platonic",
  "version": "1.0.2",
  "description": "General-purpose archetype directory reification",
  "keywords": [
    "archetype",
    "reification",
    "library",
    "general-purpose",
    "static",
    "template",
    "ejs"
  ],
  "homepage": "https://github.com/giancosta86/platonic#readme",
  "bugs": "https://github.com/giancosta86/platonic/issues",
  "license": "MIT",
  "author": {
    "name": "Gianluca Costa",
    "email": "gianluca@gianlucacosta.info",
    "url": "https://gianlucacosta.info/"
  },
  "files": [],
  "main": "dist",
  "types": "dist",
  "repository": "giancosta86/platonic",
  "scripts": {
    "clean": "rimraf dist coverage giancosta86-*.tgz",
    "clean:all": "yarn clean && rimraf node_modules",
    "format": "prettier --write .",
    "check-all": "yarn check-format && yarn lint && yarn test",
    "check-format": "prettier --check .",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx,.cjs,.mjs",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "build": "yarn compile-ts",
    "compile-ts": "tsc",
    "prepack": "yarn check-all && yarn build",
    "watch": "yarn compile-ts --watch"
  },
  "dependencies": {
    "ejs": "^3.1.8",
    "glob": "^8.1.0",
    "immutable": "^4.2.2",
    "recursive-copy": "^2.0.14"
  },
  "devDependencies": {
    "@giancosta86/more-jest-io": "^2.0.1",
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@types/ejs": "^3.1.2",
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^16",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16.19.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
